{
    "name": "college-quiz-app",
    "image": "vsc-dc/college-quiz-app",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "forwardPorts": [
        3000,
        8000
    ],
    "remoteUser": "vscode",
    "updateRemoteUserUID": false,
    "customizations": {
        "vscode": {
            "extensions": [
                "bmewburn.vscode-intelephense-client",
                "mehedidracula.php-namespace-resolver",
                "laravel.vscode-laravel"
            ]
        }
    }
}
